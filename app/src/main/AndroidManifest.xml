<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.mathup">

    <application
        android:allowBackup="true"
        android:icon="@drawable/mathuplogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mathuplogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Theme">
        <activity android:name=".TeachersLogin"></activity>
        <activity android:name=".SignUp" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Status"
            android:label="@string/title_activity_status"
            android:theme="@style/Theme.Theme.NoActionBar" />
        <activity android:name=".MainActivity"></activity>
        <activity
            android:name=".MathActivity"
            android:label="@string/app_name" />
    </application>

</manifest>